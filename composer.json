{
    "name": "julienmru/laravel-partialcache",
    "description": "Blade directive to cache rendered partials in laravel",
    "keywords": [
        "julienmru",
        "spatie",
        "cache",
        "partial",
        "view"
    ],
    "homepage": "https://github.com/julienmru/laravel-partialcache",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.2.5"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "JulienMru\\PartialCache\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JulienMru\\PartialCache\\PartialCacheServiceProvider"
            ],
            "aliases": {
                "PartialCache": "JulienMru\\PartialCache\\PartialCacheFacade"
            }
        }
    }
}
